<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:HotelAdmin.WebView.Maui"
             xmlns:web="clr-namespace:HotelAdmin.WebView.Components;assembly=HotelAdmin.WebView"
             x:Class="HotelAdmin.WebView.Maui.MainPage">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type web:Routes}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
